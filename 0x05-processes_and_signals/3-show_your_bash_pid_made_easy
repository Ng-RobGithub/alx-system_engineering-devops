#!/usr/bin/env bash
# Get the list of process IDs (PIDs) for processes containing "bash" in their name
pids=$(pgrep -f bash)

# Iterate through the PIDs and display their corresponding process names
for pid in $pids; do
	process_name=$(ps -p "$pid" -o comm=)
	echo "PID: $pid, Process Name: $process_name"
done
